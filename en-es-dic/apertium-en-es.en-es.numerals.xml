<section id="numerals" type="standard">
   
   <!-- 
      ******************************
      (num) group
      ******************************
    -->
   <e>
      <re>[0-9]+([.,][0-9]+)?</re>
      <p>
         <l><s n="num"/></l> 
         <r><s n="num"/></r> 
      </p>
   </e>
   
   <e>
      <re>[a-z.]+@[a-z.]+</re>
      <p>
         <l><s n="num"/></l> 
         <r><s n="num"/></r> 
      </p>
   </e>
   
   <e>
      <re>((www.)[a-z\-]+[.][a-z]+)</re>
      <p>
         <l><s n="num"/></l> 
         <r><s n="num"/></r> 
      </p>
   </e>
   
   <e>
      <re>[0-9]+([.,][0-9]+)?%</re>
      <p>
         <l><s n="num"/><s n="percent"/></l> 
         <r><s n="num"/><s n="percent"/></r> 
      </p>
   </e>



